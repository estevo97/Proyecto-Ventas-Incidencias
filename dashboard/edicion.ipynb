   "source": [
    "# pip install pypdf\n",
    "from pypdf import PdfReader, PdfWriter\n",
    "\n",
    "reader = PdfReader(\"informe_ejecutivo.pdf\")\n",
    "writer = PdfWriter()\n",
    "for page in reader.pages[1:]:\n",
    "    writer.add_page(page)\n",
    "with open(\"dashboard_no_first.pdf\", \"wb\") as f:\n",
    "    writer.write(f)"
   ]
  }
